generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  fullName  String   // ðŸ†• Ad Soyad
  userName  String   @unique // ðŸ†• KullanÄ±cÄ± AdÄ±
  email     String   @unique
  password  String
  bio       String?
  createdAt DateTime @default(now())
}
